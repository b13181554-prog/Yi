entrypoint = "index.js"
modules = ["nodejs-22", "postgresql-16"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"
packages = ["redis"]

[deployment]
run = ["bash", "./start.sh"]
deploymentTarget = "autoscale"
ignorePorts = false

[agent]
expertMode = true
integrations = ["javascript_openai:1.0.0"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36153
externalPort = 3002

